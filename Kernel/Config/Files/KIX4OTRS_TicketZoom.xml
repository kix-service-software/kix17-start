                <Item Key="Description">Edit this tickets dynamic fields!</Item>
                <Item Key="Block">ContentItem</Item>
                <Item Key="Permission">rw</Item>
                <Item Key="CountMethod"></Item>
            </Hash>
        </Setting>
    </ConfigItem>
