    <!-- POSTMASTER FILTER CONFIGURATION -->
    <ConfigItem Name="PostMaster::PreFilterModule###0001-SystemMonitoring" Required="0" Valid="1">
        <Description Translatable="1">Basic mail interface to System Monitoring Suites. Use this block if the filter should run AFTER PostMasterFilter.</Description>
        <Group>SystemMonitoringX</Group>
        <SubGroup>Core::PostMaster</SubGroup>
        <Setting>
            <Hash>
            <!-- if new keys are updated or to be added in SystemMonitoringX.xml, do the same in the fallback-configuration in Kernel/System/PostMaster/Filter/SystemMonitoringX.pm -->
                <Item Key="Module">Kernel::System::PostMaster::Filter::SystemMonitoringX</Item>
                <Item Key="DynamicFieldContent::Ticket">Host,Service,Address,Alias</Item>
                <Item Key="DynamicFieldContent::Article">State</Item>

                <Item Key="DynamicFieldAlias">SysMonXAlias</Item>
                <Item Key="DynamicFieldAddress">SysMonXAddress</Item>
                <Item Key="DynamicFieldHost">SysMonXHost</Item>
                <Item Key="DynamicFieldService">SysMonXService</Item>
                <Item Key="DynamicFieldState">SysMonXState</Item>

                <Item Key="AcknowledgeName">Nagios1</Item>
                <Item Key="OTRSCreateTicketType">Incident</Item>
                <Item Key="OTRSCreateTicketQueue"></Item>
                <Item Key="OTRSCreateTicketState"></Item>
                <Item Key="OTRSCreateTicketService"></Item>
                <Item Key="OTRSCreateTicketSLA"></Item>
                <Item Key="OTRSCreateSenderType">system</Item>
                <Item Key="OTRSCreateArticleType">note-report</Item>

                <Item Key="CloseNotIfLocked">0</Item>
                <Item Key="StopAfterMatch">1</Item>

                <Item Key="AddressRegExp">\s*Address:\s+(.*)\s*</Item>
                <Item Key="AliasRegExp">\s*Alias:\s+(.*)\s*</Item>
                <Item Key="FromAddressRegExp">nagios@example.com</Item>
                <Item Key="ToAddressRegExp">otrs@example.com</Item>
                <Item Key="StateRegExp">\s*State:\s+(\S+)</Item>
                <Item Key="HostRegExp">\s*Host:\s+(.*)\s*</Item>
                <Item Key="ServiceRegExp">\s*Service:\s+(.*)\s*</Item>
                <Item Key="NewTicketRegExp">CRITICAL|DOWN|WARNING</Item>
                <Item Key="CloseTicketRegExp">OK|UP</Item>
                <Item Key="CloseActionState">closed successful</Item>
                <Item Key="ClosePendingTime">172800</Item>
                <Item Key="DefaultService">Host</Item>
            </Hash>
        </Setting>
    </ConfigItem>

