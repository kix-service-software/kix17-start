    <ConfigItem Name="Ticket::EventModulePost###9-SystemMonitoringAcknowledgeX" Required="1" Valid="1">
        <Description Translatable="1">Ticket event module to send an acknowledge to registered system monitoring tools.</Description>
        <Group>SystemMonitoringX</Group>
