<div style="max-height:150px;overflow:auto">
<table class="DataTable">
    <tbody>
[% RenderBlockStart("ContentSmallOverlayOverviewRow") %]
        <tr>
            <td>
                <a href="#" title="[% Data.Subject | html %]" onclick="alert('[% Data.Message | html %]');return false;">[% Data.Subject | html %]</a>
            </td>
        </tr>
[% RenderBlockEnd("ContentSmallOverlayOverviewRow") %]
[% RenderBlockStart("ContentSmallOverlayOverviewNone") %]
        <tr>
            <td>
                [% Translate("none") | html %]
            </td>
        </tr>
[% RenderBlockEnd("ContentSmallOverlayOverviewNone") %]
    </tbody>
</table>
</div>
